@prefix cs: <http://rdf.chemspider.com/void-example.rdf#>.
@prefix dcterms: <http://purl.org/dc/terms/>.
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix pav: <http://purl.org/pav/2.0/>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix skos: <http://www.w3.org/2004/02/skos/core#>.
@prefix void: <http://rdfs.org/ns/void#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.

# Metadata about this file
<http://rdf.chemspider.com/chemspider-drugbank-example.rdf> 
	a void:DatasetDescription;
	dcterms:title "ChemSpider to DrugBank Linkset VoID Description"^^xsd:string;
	dcterms:description 
		"""This is an example VoID description for a ChemSpider linkset. 
		   The linkset relates ChemSpider identifiers with DrugBank identifiers.
		   The links have been generated as the chemicals share the same structure."""^^xsd:string;
	pav:createdBy [ 
		a foaf:Person ; 
		foaf:name "Alasdair Gray"^^xsd:string . 
		];
	pav:createdOn "2012-05-03"^^xsd:date;
	pav:derivedFrom <http://rdf.chemspider.com/void.rdf>;
	foaf:primaryTopic :chemSpiderDataset;
	.
	
# Additional information that is not declared in the ChemSpider VoID file
cs:chemSpiderDataset_drugbank_subset
	# declare that the linkset is a subset of ChemSpider
	void:subset :chemSpider_drugbank_linkset;
	.
	
# Need to declare dataset metadata about DrugBank as no VoID file
## Note this is just a minimal amount of information for the IMS, 
## it is not a complete VoID description
:drugbank_dataset
	a void:Dataset;
	foaf:homepage <http://www4.wiwiss.fu-berlin.de/drugbank/>;
	foaf:page <http://www.drugbank.ca/>;
	dcterms:title "DrugBank";
	dcterms:description "A database containing information about drugs.";
	pav:importedOn ""^^xsd:date;
	pav:importedFrom <http://www4.wiwiss.fu-berlin.de/drugbank/drugbank_dump.nt.bz2>;
	pav:importedBy [
		a foaf:Person;
		foaf:name "Antonis Loizou"^^xsd:string.
		];
	void:uriSpace "http://www4.wiwiss.fu-berlin.de/drugbank/resource/drugs/"^^xsd:string;
	.
	
# Description of the linkset from ChemSpider to DrugBank
:chemSpider_drugbank_linkset
	a void:Linkset;
	dcterms:title "ChemSpider DrugBank Linkset"^^xsd:string;
	dcterms:description "Linkset relating ChemSpider compounds to DrugBank drugs."^^xsd:string;
	## I am showing a situation in which the linkset is published under a different license
	dcterms:license <http://www.opendatacommons.org/licenses/odbl/> ;
	# Provenance metadata
	pav:authoredBy <http://www.chemspider.com/>;
	pav:authoredOn "2012-02-23"^^xsd:date;
	pav:createdBy [ 
		a foaf:Person ; 
		foaf:name "Alasdair Gray"^^xsd:string . 
		];
	pav:createdOn "2012-02-23"^^xsd:date;
	# Linkset description
	## Link back to ChemSpider VoID file
	void:subjectsTarget :chemSpiderDataset_drugbank_subset;
	## As no DrugBank VoID file, need to declare, by linking to above declaration
	void:objectsTarget :drugbank_dataset;
	void:linkPredicate skos:exactMatch;
	void:triples "6428"^^xsd:nonNegativeInteger;
	# Location of the triples
	## Note that the triples would not contain the void header information as 
	## that is in this file. I have assumed the same location as the subset 
	## above, but this needs to change to a correct location.
	## The file containing the triples should contain the following backlink
	## |uriOfTheData| void:inDataset <http://rdf.chemspider.com/void-example.rdf#chemSpider_drugbank_linkset>.
	void:dataDump <https://www.dropbox.com/s/1slf6m5qi4gr2cc/DrugBank20120426.zip>;
	.