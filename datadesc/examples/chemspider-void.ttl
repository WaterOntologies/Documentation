# Description of the ChemSpider dataset
:chemSpiderDataset 
	# Declare as a dataset
	a void:Dataset;
	# Provide links to the datasets home page
	## Is the rdf representation different from the dataset?
	foaf:homepage <http://rdf.chemspider.com/>;
	foaf:page <http://www.chemspider.com/>;
	# Basic dublin core metadata
	dcterms:title "ChemSpider"^^xsd:string;
	dcterms:description "ChemSpider's Public Dataset"^^xsd:string;
	dcterms:source <http://www.chemspider.com/>;
	# Contact information
	dcterms:publisher <http://www.chemspider.com/>;	
	# License declaration	
	dcterms:license <http://www.chemspider.com/Disclaimer.aspx>;
	# Datasets subject for categorizing, can have multiple
	dcterms:subject <http://dbpedia.org/resource/Molecule>;
	# Technical features
	void:feature <http://www.w3.org/ns/formats/RDF_XML>;
	# Access metadata
	void:sparqlEndpoint <http://rdf.chemspider.com/sparql>;
	## Should include details of data dumps
	# Structural Metadata
	# Example resources, can have multiple
	void:exampleResource <http://rdf.chemspider.com/2157>;
	void:uriSpace "http://rdf.chemspider.com/"^^xsd:string;
	# Vocabularies used
	void:vocabulary <http://purl.org/dc/elements/1.1/>,
		dcterms:,
		<http://www.openarchives.org/ore/terms/>,
		<http://www.polymerinformatics.com/ChemAxiom/ChemDomain.owl#>,
		<http://xmlns.com/foaf/0.1/>;
	# Partitioned datasets
	void:subset :chemSpiderDataset_chembl_subset,
		:chemSpiderDataset_drugbank_subset;
	# Dataset statistics
	void:triples "1157624328"^^xsd:nonNegativeInteger;
	.

